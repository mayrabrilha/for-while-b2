#include <stdio.h>

int main() {
    int numeros[5];  
    int i;           
    
    for (i = 0; i < 5; i++) {
        printf("Digite o número %d: ", i + 1);  
        scanf("%d", &numeros[i]);
    }

    
    printf("Você inseriu os seguintes números:\n");
    for (i = 0; i < 5; i++) {
        printf("%d ", numeros[i]);  
    }
    printf("\n");  

    return 0; 
}
